<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#0B5C55"
                android:endColor="#3CAFA4"
                android:angle="270"/>
            <corners android:radius="30dp"/>
        </shape>
    </item>
    <item
        android:left="10dp"
        android:right="10dp"
        android:top="10dp"
        android:bottom="10dp">
    <shape android:shape="rectangle">
        <solid android:color="#35D8C9"/>
        <corners android:radius="10dp"/>
    </shape>
</item>
    </layer-list>